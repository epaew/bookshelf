{
  "name": "@bookshelf/api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/",
    "db:migration:apply:development": "prisma migrate dev --preview-feature",
    "db:migration:apply:production": "prisma migrate deploy --preview-feature",
    "db:migration:generate": "prisma migrate dev --create-only --preview-feature",
    "db:migration:reset": "prisma migrate reset --preview-feature",
    "dev": "nodemon --files src/index.ts",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "4.10.1",
    "apollo-server-express": "^3.1.2",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "nanoid": "^3.1.25",
    "pg": "^8.5.1",
    "prisma": "2.30.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.7.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
